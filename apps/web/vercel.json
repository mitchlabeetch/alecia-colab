{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://alecia.fr"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://alecia.fr https://*.alecia.fr https://*.vercel.app"
        }
      ]
    }
  ]
}