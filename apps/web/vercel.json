{
  "buildCommand": "cd ../.. && pnpm build",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && pnpm install",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://alecia.fr https://alecia.markets"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://alecia.fr https://*.alecia.fr https://alecia.markets https://*.alecia.markets https://*.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://alecia.fr"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/embed",
      "destination": "/?embed=true"
    }
  ]
}